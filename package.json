    "typescript": "~3.7.2",
    "uuid": "*"
  },
  "scripts": {
    "start": "PORT=9090 react-scripts start",
    "build": "react-scripts build && npm run build:css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss assets/tailwind.css -o src/styles/main.css",
